<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.alternaleaf.org/ontology#"
    xml:base="http://www.alternaleaf.org/ontology"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

  <!-- Classes -->
  <rdfs:Class rdf:about="#PatientPortal"/>
  <rdfs:Class rdf:about="#Patient"/>
  <rdfs:Class rdf:about="#Prescription"/>
  <rdfs:Class rdf:about="#Medication"/>
  <rdfs:Class rdf:about="#Appointment"/>
  <rdfs:Class rdf:about="#Email"/>
  <rdfs:Class rdf:about="#AccountActivation"/>
  <rdfs:Class rdf:about="#Login"/>
  <rdfs:Class rdf:about="#PasswordReset"/>
  <rdfs:Class rdf:about="#UserProfile"/>
  <rdfs:Class rdf:about="#Link"/>

  <!-- Properties -->
  <rdf:Property rdf:about="#hasPrescription">
    <rdfs:domain rdf:resource="#Patient"/>
    <rdfs:range rdf:resource="#Prescription"/>
  </rdf:Property>

  <rdf:Property rdf:about="#hasMedication">
    <rdfs:domain rdf:resource="#Prescription"/>
    <rdfs:range rdf:resource="#Medication"/>
  </rdf:Property>

  <rdf:Property rdf:about="#booksAppointment">
    <rdfs:domain rdf:resource="#Patient"/>
    <rdfs:range rdf:resource="#Appointment"/>
  </rdf:Property>

  <rdf:Property rdf:about="#receivesEmail">
    <rdfs:domain rdf:resource="#Patient"/>
    <rdfs:range rdf:resource="#Email"/>
  </rdf:Property>

  <rdf:Property rdf:about="#emailHasSubject">
    <rdfs:domain rdf:resource="#Email"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <rdf:Property rdf:about="#emailContainsLink">
    <rdfs:domain rdf:resource="#Email"/>
    <rdfs:range rdf:resource="#Link"/>
  </rdf:Property>

  <rdf:Property rdf:about="#activatesAccountVia">
    <rdfs:domain rdf:resource="#Patient"/>
    <rdfs:range rdf:resource="#AccountActivation"/>
  </rdf:Property>

  <rdf:Property rdf:about="#hasLogin">
    <rdfs:domain rdf:resource="#Patient"/>
    <rdfs:range rdf:resource="#Login"/>
  </rdf:Property>

  <rdf:Property rdf:about="#canResetPasswordVia">
    <rdfs:domain rdf:resource="#Patient"/>
    <rdfs:range rdf:resource="#PasswordReset"/>
  </rdf:Property>

  <rdf:Property rdf:about="#loginUsesEmail">
    <rdfs:domain rdf:resource="#Login"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Property>

  <rdf:Property rdf:about="#loginLinksToPortal">
    <rdfs:domain rdf:resource="#Login"/>
    <rdfs:range rdf:resource="#PatientPortal"/>
  </rdf:Property>

  <rdf:Property rdf:about="#refersTo">
    <rdfs:domain rdf:resource="#PatientPortal"/>
    <rdfs:range rdf:resource="#PatientPortal"/>
  </rdf:Property>

  <!-- Individuals -->
  <rdf:Description rdf:about="#AlternaleafPatientPortal">
    <rdf:type rdf:resource="#PatientPortal"/>
  </rdf:Description>

  <rdf:Description rdf:about="#ActivateAccountEmail">
    <rdf:type rdf:resource="#Email"/>
    <emailHasSubject>Action required: Activate your account</emailHasSubject>
    <emailContainsLink rdf:resource="#CreatePasswordLink"/>
  </rdf:Description>

  <rdf:Description rdf:about="#CreatePasswordLink">
    <rdf:type rdf:resource="#Link"/>
  </rdf:Description>

  <rdf:Description rdf:about="#PatientUserLogin">
    <rdf:type rdf:resource="#Login"/>
    <loginUsesEmail>patient@example.com</loginUsesEmail>
    <loginLinksToPortal rdf:resource="#AlternaleafPatientPortal"/>
  </rdf:Description>

  <rdf:Description rdf:about="#PasswordResetLink">
    <rdf:type rdf:resource="#PasswordReset"/>
  </rdf:Description>

  <rdf:Description rdf:about="#CircuitPortal">
    <rdf:type rdf:resource="#PatientPortal"/>
  </rdf:Description>

  <rdf:Description rdf:about="#ExamplePatient">
    <rdf:type rdf:resource="#Patient"/>
    <hasPrescription rdf:resource="#ExamplePrescription"/>
    <booksAppointment rdf:resource="#ExampleAppointment"/>
    <receivesEmail rdf:resource="#ActivateAccountEmail"/>
    <activatesAccountVia rdf:resource="#AccountActivationProcess"/>
    <hasLogin rdf:resource="#PatientUserLogin"/>
    <canResetPasswordVia rdf:resource="#PasswordResetLink"/>
  </rdf:Description>

  <rdf:Description rdf:about="#ExamplePrescription">
    <rdf:type rdf:resource="#Prescription"/>
    <hasMedication rdf:resource="#ExampleMedication"/>
  </rdf:Description>

  <rdf:Description rdf:about="#ExampleMedication">
    <rdf:type rdf:resource="#Medication"/>
  </rdf:Description>

  <rdf:Description rdf:about="#ExampleAppointment">
    <rdf:type rdf:resource="#Appointment"/>
  </rdf:Description>

  <rdf:Description rdf:about="#AccountActivationProcess">
    <rdf:type rdf:resource="#AccountActivation"/>
  </rdf:Description>

</rdf:RDF>
